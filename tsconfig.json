{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "jsx": "react"
  },
  "include": [
    "src/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
